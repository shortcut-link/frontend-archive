{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "browser-cookies": "^1.2.0",
    "effector": "^20.0.0",
    "effector-react": "^20.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "react": "16.9.0",
    "react-dom": "^16.8.6",
    "react-google-recaptcha": "^2.0.0-rc.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-test-renderer": "^16.8.6",
    "react-virtualized": "^9.21.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --setupFilesAfterEnv ./config/tests/setupTests.js",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "cz": "git cz"
  },
  "proxy": "http://localhost:8080/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./config/git/commitizen.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g ./config/git/commitlint.js",
      "pre-commit": "yarn run lint && yarn test",
      "pre-push": "yarn run lint && yarn test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "commitizen": "^4.0.3",
    "cross-env": "^5.2.0",
    "cz-customizable": "^6.2.0",
    "husky": "^3.0.1",
    "jest-fetch-mock": "^2.1.2",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.18.2"
  }
}
